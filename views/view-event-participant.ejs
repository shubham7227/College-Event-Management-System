<%- include("partials/header2", {title: "Event Participant"}) %>

<body>
  <header class="site-header">
    <div class="header-bar">
      <div class="container-fluid">
        <div class="row align-items-center">
          <div class="col-10 col-lg-4">
            <h1 class="site-branding flex">
              <a href="/club-profile">Event Manager</a>
            </h1>
          </div>

          <div class="col-2 col-lg-8">
            <nav class="all-header">
              <a class="boton" href="/club-profile">PROFILE</a>
              <a class="boton" href="/logout">LOGOUT</a>
            </nav>
          </div>
        </div>
      </div>
    </div>
  </header>

  <div class="event-reg-form">
    <div class="main-content">
      <div class="container">
        <div class="entry-header">
          <div class="entry-title">
            <h2>Event Participants</h2>
          </div>
        </div>
      </div>
    </div>
    <div class="event-reg">
      <table id="participants">
        <tr>
          <th>S.No.</th>
          <th>Name</th>
          <th>Registration Number</th>
          <th>E-mail</th>
          <th>Phone Number</th>
        </tr>
        <% i=1 %> <% participant_lists.forEach(participant => { %>
        <tr>
          <td><%= i %></td>
          <td>
            <%= participant.fname %> <%= participant.mname %> <%=participant.lname %>
          </td>
          <td><%= participant.regno %></td>
          <td><%= participant.email %></td>
          <td><%= participant.phno %></td>
        </tr>
        <% i=i+1 %> <% }); %>
      </table>
    </div>
    <br />
    <a href="/registered-event">
      <div class="btn btn-primary">BACK</div>
    </a>
  </div>

<%- include("partials/footer2") %>